---
import PageLayout from '../layouts/PageLayout.astro';

const pageTitle = "SEO Services - Outcome Digital Marketing Ltd";
const pageDescription = "Comprehensive SEO services including SEO audits, content creation, technical optimization, and local SEO to transform your website into a ranking asset.";
---

<PageLayout title={pageTitle} description={pageDescription} currentPath="/services">
  <!-- Hero Section -->
  <section class="relative bg-gradient-to-br from-gray-900 via-gray-900 to-gray-800 text-white py-20 overflow-hidden">
    <!-- Decorative gradient overlay -->
    <div class="absolute inset-0 opacity-10">
      <div class="absolute top-0 left-0 w-96 h-96 bg-primary rounded-full blur-3xl"></div>
      <div class="absolute bottom-0 right-0 w-96 h-96 bg-accent rounded-full blur-3xl"></div>
    </div>

    <div class="relative container">
      <div class="max-w-4xl mx-auto text-center">
        <h1 class="!text-white text-4xl md:text-6xl font-bold mb-6">
          Our SEO Services
        </h1>
        <p class="!text-gray-300 text-xl md:text-2xl mb-8 leading-relaxed">
          Comprehensive solutions designed to transform your website's search performance and drive measurable results.
        </p>
        <a
          href="/contact"
          class="inline-flex items-center gap-2 px-8 py-4 bg-accent text-white font-semibold rounded-xl hover:bg-accent-dark transition-all duration-300 shadow-lg hover:shadow-xl hover:scale-105"
        >
          <span>Get Free Consultation</span>
          <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7l5 5m0 0l-5 5m5-5H6" />
          </svg>
        </a>
      </div>
    </div>
  </section>

  <!-- Services Introduction -->
  <section class="py-20 bg-white">
    <div class="container">
      <div class="max-w-4xl mx-auto text-center">
        <h2 class="text-3xl md:text-4xl font-bold text-gray-900 mb-6">
          Transform Your Website with Expert SEO Services
        </h2>
        <p class="text-xl text-gray-600 leading-relaxed">
          At Outcome Digital Marketing, we specialize in turning underperforming websites into powerful ranking assets.
          Our comprehensive SEO services address every aspect of your online presence to ensure you not only rank higher
          but also attract qualified traffic that converts.
        </p>
      </div>
    </div>
  </section>

  <!-- Detailed Services -->
